@model IEnumerable<GameStoreDAL.Entities.Order>
@{
    ViewBag.Title = "GetAllOrders";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

    <div class="container mt-4 ">

        @if (Model.Count() > 0)
        {
            <h1 class="font-weight-bolder w-100"> Orders:</h1>
            Html.RenderPartial("Orders");
        }
        else
        {
            <p>No orders</p>
        }
    </div>


